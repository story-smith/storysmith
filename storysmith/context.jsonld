{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://story-smith.github.io/storysmith/ontology#",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "Character": "schema:Person",
    "Place": "schema:Place",
    "TimePoint": "schema:Event",
    "Event": "story:Event",
    "Scene": "story:Scene",
    "subject": {
      "@id": "rdf:subject",
      "@type": "@id"
    },
    "predicate": {
      "@id": "rdf:predicate",
      "@type": "xsd:string"
    },
    "object": {
      "@id": "rdf:object",
      "@type": "@id"
    },
    "location": {
      "@id": "schema:location",
      "@type": "@id"
    },
    "time": {
      "@id": "schema:startTime",
      "@type": "@id"
    },
    "description": {
      "@id": "schema:description",
      "@type": "xsd:string"
    },
    "episode": {
      "@id": "story:episode",
      "@type": "xsd:string"
    },
    "label": "rdfs:label"
  }
}
